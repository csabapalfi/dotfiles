{
  "*": {
    "release-notes": {
      "viewedVersion": "0.89.0"
    },
    "editor": {
      "fontSize": 22,
      "showInvisibles": true,
      "invisibles": {},
      "softWrap": true
    },
    "core": {
      "ignoredNames": [
        ".git",
        ".svn",
        ".DS_Store"
      ],
      "disabledPackages": [
        "welcome",
        "wrap-guide",
        "styleguide",
        "release-notes",
        "package-generator",
        "metrics",
        "link",
        "language-xml",
        "language-toml",
        "language-text",
        "language-ruby-on-rails",
        "language-php",
        "language-perl",
        "feedback",
        "exception-reporting",
        "background-tips",
        "autoflow",
        "bookmarks",
        "encoding-selector",
        "language-csharp",
        "open-on-github",
        "minimap-find-and-replace"
      ],
      "themes": [
        "seti-ui",
        "atom-dark-syntax"
      ],
      "audioBeep": false
    },
    "linter-jshint": {},
    "linter": {},
    "autosave": {
      "enabled": true
    },
    "minimap": {
      "plugins": {
        "find-and-replace": true,
        "git-diff": true,
        "linter": true
      }
    },
    "linter-eslint": {
      "disableWhenNoEslintrcFileInPath": true
    }
  },
  ".source.yaml": {
    "editor": {
      "tabLength": 2
    }
  }
}
