{
  "*": {
    "editor": {
      "fontSize": 22,
      "showInvisibles": true,
      "invisibles": {}
    },
    "core": {
      "ignoredNames": [
        ".git",
        ".svn",
        ".DS_Store"
      ],
      "disabledPackages": [
        "welcome",
        "wrap-guide",
        "styleguide",
        "release-notes",
        "package-generator",
        "metrics",
        "link",
        "language-xml",
        "language-toml",
        "language-text",
        "language-ruby-on-rails",
        "language-php",
        "language-perl",
        "feedback",
        "exception-reporting",
        "background-tips",
        "autoflow",
        "bookmarks",
        "encoding-selector",
        "language-csharp",
        "open-on-github",
        "autocomplete-snippets"
      ],
      "themes": [
        "seti-ui",
        "atom-dark-syntax"
      ],
      "audioBeep": false
    },
    "linter": {
      "showErrorPanel": false
    },
    "autosave": {
      "enabled": true
    },
    "linter-eslint": {
      "disableWhenNoEslintrcFileInPath": true
    },
    "tree-view": {
      "hideVcsIgnoredFiles": false
    }
  }
}
