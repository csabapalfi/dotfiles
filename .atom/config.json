{
  "*": {
    "atom-package-deps": {
      "ignored": []
    },
    "autosave": {
      "enabled": true
    },
    "core": {
      "audioBeep": false,
      "disabledPackages": [
        "welcome",
        "wrap-guide",
        "styleguide",
        "release-notes",
        "package-generator",
        "metrics",
        "link",
        "language-xml",
        "language-toml",
        "language-text",
        "language-ruby-on-rails",
        "language-php",
        "language-perl",
        "feedback",
        "exception-reporting",
        "background-tips",
        "autoflow",
        "bookmarks",
        "encoding-selector",
        "language-csharp",
        "open-on-github",
        "autocomplete-snippets"
      ],
      "ignoredNames": [
        ".git",
        ".svn",
        ".DS_Store"
      ],
      "telemetryConsent": "limited",
      "themes": [
        "atom-dark-ui",
        "atom-dark-syntax"
      ],
      "titleBar": "custom"
    },
    "editor": {
      "fontSize": 21,
      "invisibles": {},
      "showInvisibles": true
    },
    "linter": {},
    "linter-eslint": {
      "disableWhenNoEslintrcFileInPath": true
    },
    "linter-js-standard": {
      "checkStyleDevDependencies": true
    },
    "linter-ui-default": {},
    "prettier-atom": {
      "formatOnSaveOptions": {
        "showInStatusBar": true
      },
      "useEslint": true
    },
    "standard-formatter": {
      "checkStyleDevDependencies": true,
      "formatOnSave": true
    },
    "tree-view": {}
  }
}
